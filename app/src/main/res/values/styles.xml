<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="BaseTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    
        <item name="android:windowBackground">@color/colorAccent</item>
    </style>
    
    
    <style name="LeftRightAnim">
        <item name="android:windowEnterAnimation">@anim/anim_right_in</item>
        <item name="android:windowExitAnimation">@anim/anim_left_out</item>
    </style>
    
   
    <style name="EditText_448aff" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/text_666666</item>
        <item name="colorControlActivated">@color/colorAccent</item>
    </style>
    
    <style name="EditText_7c4dff" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/text_666666</item>
        <item name="colorControlActivated">@color/color_7c4dff</item>
    </style>


</resources>
